<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/webflow
                          http://www.springframework.org/schema/webflow/spring-webflow.xsd">

  <var name="perfonalInfo" class="ph.com.usegutierrez.simplespringwebflow.dto.PersonalInfo" />

  <!-- start state -->
  <view-state id="signup" view="flows/signup/signup">

  </view-state>

  <view-state id="home" view="externalRedirect:contextRelative:/" />

  <global-transitions>
    <transition on="home" to="home"></transition>
  </global-transitions>

</flow>